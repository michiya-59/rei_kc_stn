default: &default
  adapter: postgresql
  encoding: unicode
  username: <%= ENV['DB_USERNAME'] %>
  password: <%= ENV['DB_PASSWORD'] %>
  host: <%= ENV['DB_HOST'] %>
  pool: 5

development:
  <<: *default
  database: rei_kc_stn_development

test:
  <<: *default
  database: rei_kc_stn_test

production:
  <<: *default
  database: rei_kc_stn_production



